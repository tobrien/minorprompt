{
    "name": "@tobrien/minorprompt",
    "version": "0.0.3",
    "description": "Get it together, and organize your prompts.",
    "type": "module",
    "module": "./dist/main.js",
    "types": "./dist/main.d.ts",
    "packageManager": "pnpm@10.10.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tobrien/minorprompt.git"
    },
    "exports": {
        ".": {
            "import": "./dist/minorPrompt.js",
            "types": "./dist/minorPrompt.d.ts"
        }
    },
    "scripts": {
        "build": "vite build",
        "start": "dist/main.js",
        "dev": "vite",
        "watch": "vite build --watch",
        "test": "pnpm run test:coverage && pnpm run test:readme",
        "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
        "test:readme": "markdown-doctest -c .markdown-doctest-setup.mjs README.md",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run clean && npm run build"
    },
    "keywords": [
        "prompt",
        "llm",
        "genai",
        "openai"
    ],
    "author": "Tim O'Brien <tobrien@discursive.com>",
    "license": "Apache-2.0",
    "devDependencies": {
        "@babel/core": "^7.27.1",
        "@babel/plugin-transform-modules-commonjs": "^7.27.1",
        "@babel/plugin-transform-runtime": "^7.27.1",
        "@babel/plugin-transform-typescript": "^7.27.1",
        "@babel/preset-typescript": "^7.27.1",
        "@babel/runtime": "^7.27.1",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.26.0",
        "@jest/globals": "^29.7.0",
        "@rollup/plugin-replace": "^6.0.2",
        "@swc/core": "^1.11.24",
        "@tobrien/markdown-doctest": "^1.2.1",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.15.17",
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "eslint": "^9.26.0",
        "eslint-plugin-import": "^2.31.0",
        "globals": "^16.1.0",
        "jest": "^29.7.0",
        "rollup-plugin-preserve-shebang": "^1.0.1",
        "ts-jest": "^29.3.2",
        "typescript": "^5.8.3",
        "vite": "^6.3.5",
        "vite-plugin-dts": "^4.5.3",
        "vite-plugin-node": "^5.0.1"
    },
    "dependencies": {
        "glob": "^11.0.2",
        "marked": "^15.0.11"
    }
}